file = {:Users/oliver-d/Library/Application Support/Mendeley Desktop/Downloaded/Fougner et al. - 2011 - Resolving the limb position effect in myoelectric pattern recognition.pdf:pdf},
isbn = {1534-4320},
issn = {15344320},
journal = {IEEE Trans. Neural Syst. Rehabil. Eng.},
keywords = {Accelerometer,electromyography,prosthetic hands,prosthetics},
