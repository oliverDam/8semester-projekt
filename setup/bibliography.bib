isbn = {9781467319690},
issn = {19483546},
journal = {Int. IEEE/EMBS Conf. Neural Eng. NER},
pages = {359--362},
title = {{Evaluating upper-limb EMG-prosthesis user performance by combining psychometric measures and classification-rates}},
